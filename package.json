{
  "name": "flowrite",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "dependencies": {
    "bulma": "^0.8.0",
    "electron-is-dev": "^1.1.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-scripts": "3.2.0",
    "electron-store": "5.0.0"
  },
  "scripts": {
    "test": "eslint src/*.js && rescripts test",
    "start": "rescripts start",
    "build": "rescripts build",
    "eject": "react-scripts eject",
    "dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "rescripts": [
    "env"
  ],
  "homepage": "./",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rescripts/cli": "0.0.12",
    "@rescripts/rescript-env": "0.0.10",
    "babel-eslint": "^10.0.3",
    "concurrently": "^5.0.0",
    "electron": "^6.0.12",
    "electron-builder": "^21.2.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "wait-on": "^3.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flowriteapp/flowrite.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/flowriteapp/flowrite/issues"
  }
}
